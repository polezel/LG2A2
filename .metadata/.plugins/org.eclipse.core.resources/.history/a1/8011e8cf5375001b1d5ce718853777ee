package lista2;
import java.math.*;

public class Conta {

	private String nome;
	private BigDecimal saldo;

	public Conta(String nome) {
		
		this.nome = nome;
		saldo = new BigDecimal("0.00");
				
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public BigDecimal getSaldo() {
		return saldo;
	}

	public void setSaldo(BigDecimal saldo) {
		this.saldo = saldo;
	}
	
}
